# Variables de entorno para Vite
# Nota: Las variables deben comenzar con VITE_ para ser accesibles en el navegador

# URL base de la API del backend
VITE_API_URL=http://localhost:8000/api
# URL alternativa para API (puerto 8001)
VITE_API_URL_ALT=http://localhost:8001/api
# URL alternativa para API (puerto 8003)
VITE_API_URL_ALT2=http://localhost:8003/api
# URL para WebSockets
VITE_API_WS_URL=ws://localhost:8000
# URL alternativa para WebSockets (puerto 8001)
VITE_API_WS_URL_ALT=ws://localhost:8001
# URL alternativa para WebSockets (puerto 8003)
VITE_API_WS_URL_ALT2=ws://localhost:8003

# Otras configuraciones
VITE_APP_NAME=IFAP - Instituto de Formación Archivística
VITE_APP_VERSION=1.0.0